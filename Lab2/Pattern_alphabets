/* Write a program to display the following pattern
A
BC
DEF
GHIJ
KLMNO
*/

public class Pattern_alphabets {

	public static void main(String[] args) {
		int i,j;
		char ch='A';
		for(i=1;i<=5;i++) //LOOP FOR ROW VALUE //
		{
			for(j=1;j<=i;j++) //LOOP FOR CLOUMN VALUE //
			{
				System.out.print(ch+"");// DISPLAY THE CHARACTER //
				ch++;
			}		
			System.out.println();
		}
	}
}
