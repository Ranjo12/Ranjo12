/* Q3)Write a program to display the following pattern

A
BC
DEF
GHIJ
KLMNO
*/

package Ranjeet;

public class Alpha_pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char i,j;
		char ch='A';
		for(i=1;i<=5;i++) //loop for print row
		{
			for(j=1;j<=i;j++)//loop for print column
			{
				System.out.print(ch++);//print char ABCD..
			}
			System.out.println();
		}

	}

}
